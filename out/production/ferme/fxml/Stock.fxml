<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<TableView xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           stylesheets="@/css/Personnels.css"
           fx:controller="com.groupeone.ferme.controllers.StockController">

  <Label text="Le Stock" styleClass="h3"/>
  <TableView fx:id="tableView">
    <columns>
      <TableColumn fx:id="numero" text="Id">
        <cellValueFactory>
          <PropertyValueFactory property="id"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="type" text="Type">
        <cellValueFactory>
          <PropertyValueFactory property="type"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="quantite" text="Quantité">
        <cellValueFactory>
          <PropertyValueFactory property="quantite"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="gerant" text="Gérant">
        <cellValueFactory>
          <PropertyValueFactory property="personnel"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="date" text="Date">
        <cellValueFactory>
          <PropertyValueFactory property="date"/>
        </cellValueFactory>
      </TableColumn>
    </columns>
  </TableView>
</TableView>
