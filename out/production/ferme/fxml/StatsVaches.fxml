<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      stylesheets="@/css/Personnels.css"
      prefHeight="500" prefWidth="500"
      fx:id="pageStats"
      fx:controller="com.groupeone.ferme.controllers.StatsVachesController">

  <Label text="Page Stats vaches" styleClass="h3"/>

  <TableView fx:id="tableView"  VBox.vgrow="ALWAYS">
    <columns>
      <TableColumn fx:id="id" text="Identifiant">
        <cellValueFactory>
          <PropertyValueFactory property="idAnimal"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="sexe" text="Sexe">
        <cellValueFactory>
          <PropertyValueFactory property="sexe"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="vivant" text="Vivant">
        <cellValueFactory>
          <PropertyValueFactory property="vivant"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="dateNaissance" text="Date de Naissance">
        <cellValueFactory>
          <PropertyValueFactory property="dateNaissance"/>
        </cellValueFactory>
      </TableColumn>
    </columns>
  </TableView>
</VBox>
