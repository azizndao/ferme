<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      stylesheets="@/css/Personnels.css"
      prefHeight="500" prefWidth="500"
      fx:controller="com.groupeone.ferme.controllers.SanteController">

  <Label text="Page sante" styleClass="h3"/>
  <TableView fx:id="tableView">
    <columns>
      <TableColumn fx:id="id" text="Id">
        <cellValueFactory>
          <PropertyValueFactory property="idAnimal"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="maladie" text="Maladie">
        <cellValueFactory>
          <PropertyValueFactory property="maladie"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="description" text="Description">
        <cellValueFactory>
          <PropertyValueFactory property="description"/>
        </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="personnel" text="Vétérinaire">
        <cellValueFactory>
          <PropertyValueFactory property="personnel"/>
        </cellValueFactory>
      </TableColumn>
    </columns>
  </TableView>
</VBox>
